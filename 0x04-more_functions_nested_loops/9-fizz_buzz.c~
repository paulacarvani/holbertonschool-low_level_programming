#include "holberton.h"
/**
 *main - entry point
 *
 * Return: Always 0.
 */
int main(void)
{
int x = 1;
while (x <= 100)
{
if (x % 3 == 0 && x % 5 == 0)
printf("FizzBuzz");
else if (x % 3 == 0)
printf("Fizz");
else if (x % 5 == 0)
printf("Buzz");
else if (x != 100)
printf(" ");
else
printf("%d", x);
x++;
}
printf("\n");
return (0);
}
